<!-- wp:list-item {"fontSize":"large"} -->
<li class="has-large-font-size"><a href="https://wordpress.org/five-for-the-future/for-individuals/" data-type="page" data-id="8">Contribute as an individual</a></li>
<!-- /wp:list-item --></ul>
<!-- /wp:list --></div>
<!-- /wp:column --></div>
<!-- /wp:columns --></div>
<!-- /wp:group -->

